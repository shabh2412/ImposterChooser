<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rishabhpanesar.imposterchooser">

    <application
        android:allowBackup="true"
        android:icon="@drawable/among_us_main_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/among_us_main_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.ImposterChooser">
        <activity android:name="com.rishabhpanesar.imposterchooser.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>